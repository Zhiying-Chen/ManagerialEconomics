   "source": [
    "import pandas as pd\n",
    "from pandas import ExcelWriter\n",
    "##   这里输入要导入txt的名字\n",
    "dic = {0:\"0913信用债成交\",1:\"0914信用债成交\",2:\"0915信用债成交\",3:\"0916信用债成交\"}\n",
    "\n",
    "\n",
    "\n",
    "\n",
    "##  设计判别函数\n",
    "def judge(j):\n",
    "    import re\n",
    "    zhmodel = re.compile(u'[\\u4e00-\\u9fa5]')\n",
    "    match = zhmodel.search(j)\n",
    "    if \".\" in j:\n",
    "        if \"S\" in j or \"IB\" in j:\n",
    "            return \"b\"\n",
    "        elif \"D\" in j or \"Y\" in j:\n",
    "            return \"a\"\n",
    "        elif \"行权\" in j or \"净价\" or \"到期\" in j:\n",
    "            return \"d\"\n",
    "        else:\n",
    "            return \"d\"\n",
    "    elif \"AA\" in j:\n",
    "        return \"e\"\n",
    "    elif j == \"100\" or j== \"2\" or j == \"3\" or j == \"6\" or j == \"4\" or j == \"5\" or j == \"7\" or j == \"8\":\n",
    "        return \"d\"\n",
    "    elif j.isnumeric():\n",
    "        return \"b\"   \n",
    "    elif j == \"行权\" or j == \"净价\" or j == \"到期\":\n",
    "        pass\n",
    "    elif match:\n",
    "        if \"休\" in j:\n",
    "            return \"a\"\n",
    "        elif \"净价\" in j:\n",
    "            return \"d\"\n",
    "        elif j == \"三位\" or j == \"四位\":\n",
    "            return \"没用的\"\n",
    "        else:\n",
    "            return \"c\"\n",
    "    elif \"GLP\" in j or \"CHNE\" in j or \"HBST\" in j or \"CHNG\" in j or \"SZMC\" in j or \"GLPR\" in j or \"DTFD\" in j:\n",
    "        return \"c\"\n",
    "    elif j == \"tkn\" or j == \"gvn\" or j == \"trd\":\n",
    "        return \"delete\"\n",
    "    else:\n",
    "        return \"a\"\n",
    "    \n",
    "\n",
    "\n",
    "\n",
    "\n",
    "\n",
    "\n",
    "\n",
    "writer = ExcelWriter('原始数据.xlsx')\n",
    "for i in range(4):\n",
    "##    导入excel\n",
    "    z = \"\" #定义一个新的字符串\n",
    "    data = pd.DataFrame(columns=range(5), index=range(10000)) #创建一个新的dataframe\n",
    "    l = 0 #代表excel中的行数\n",
    "    n = 0 #代表excel中的列数\n",
    "    x = 1 #用于查看\n",
    "\n",
    "    with open(dic[i]+\".txt\", 'r', encoding='utf-8') as f:\n",
    "        for line in f: #遍历每一行\n",
    "\n",
    "            for a in line: #遍历行里每一个字符\n",
    "                if n == 5:\n",
    "                    pass\n",
    "                else:\n",
    "                    if a != ' ' and a != '\\n' and a != '\\t': #如果字符不是空格，将字符串加入到z中\n",
    "                        z += a\n",
    "                    else: #直到遇到字符串为空格\n",
    "                        if z != \"\":\n",
    "                            if judge(z) == \"a\":\n",
    "                                data.iloc[l][0] = z #将a类型字符串加入到第l行第0列\n",
    "                            elif judge(z) == \"b\":\n",
    "                                data.iloc[l][2] = z #将b类型字符串加入到第l行第1列\n",
    "                            elif judge(z) == \"c\":\n",
    "                                data.iloc[l][1] = z #将a类型字符串加入到第l行第2列\n",
    "                            elif judge(z) == \"d\":\n",
    "                                data.iloc[l][4] = z #将b类型字符串加入到第l行第3列\n",
    "                            elif judge(z) == \"e\":\n",
    "                                data.iloc[l][3] = z #将b类型字符串加入到第l行第3列\n",
    "                        z = \"\"\n",
    "            n = 0\n",
    "            l = l + 1\n",
    "            z = \"\"\n",
    "            x += 1\n",
    "\n",
    "\n",
    "\n",
    "    ##   删空行\n",
    "    j = list()\n",
    "    for o in data.index:\n",
    "        if pd.isna(data.iloc[o,0]):\n",
    "            j.append(o)\n",
    "    data.drop(index=j,inplace=True)\n",
    "\n",
    "\n",
    "\n",
    "    ##   保存文件  \n",
    "    data.to_excel(writer,sheet_name=dic[i]) #将data用从dataframe中导入到excel中输出\n",
    "writer.save()\n"
   ]
  },
